{
	"name": "fantasymarket-app",
	"version": "0.0.1",
	"repository": "https://github.com/explodingcamera/fantasymarket-app.git",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"export": "next export",
		"deploy": "yarn run build && yarn run export",
		"test": "yarn run lint && yarn run ava",
		"lint": "eslint .",
		"ava": "ava"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test"
		}
	},
	"ava": {
		"babel": {
			"testOptions": {
				"presets": [
					"module:@ava/babel/stage-4"
				]
			}
		},
		"require": [
			"esm"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/plugin-proposal-optional-chaining": "^7.8.3",
		"@explodingcamera/eslint-config": "^2.3.0",
		"ava": "^3.3.0",
		"eslint": "^6.8.0"
	},
	"dependencies": {
		"@ava/babel": "^1.0.1",
		"@explodingcamera/next-plugin-styled-components": "^1.1.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"esm": "^3.2.25",
		"husky": "^4.2.3",
		"modern-normalize": "^0.6.0",
		"next": "^9.2.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"styled-components": "^5.0.1",
		"victory": "^34.1.1"
	}
}
